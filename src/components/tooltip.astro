---
import MyToolTip from "/src/components/react/tooltip.jsx"
const { id, label } = Astro.props;

const posts = await Astro.glob('/src/content/docs/glossary/*.md');
const filtered = posts.find(post => post.file.includes(id));
---

<MyToolTip id={id} label={label} slot="test" html={filtered.compiledContent()} client:only></MyToolTip>
